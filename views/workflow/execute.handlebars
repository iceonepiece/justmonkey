<br><br><br>
<center> <h3><strong>Template Workflow List</strong></h3></center>

<br><br><br>

<table width ="900" border = "0" align = "center">
	<tr>
		<td>
			
			<table  class="table table-bordered" >
				<tr>
					<th> Name </th>
					<th> Description </th>
				</tr>
				{{#each workflows}}
				<tr>
					<td width = "300"> 
						<div style ="font-size: 19px"><a href="/workflow/{{this._id}}/profile">{{this.name}}</a></div>
					</td>
					<td width = "300">
						<div>{{this.description}}</div>
					</td>
					
				</tr>
				{{/each}}
			</table>
		
		</td>
	</tr>
</table>
